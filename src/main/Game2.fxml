<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet blindtest.css?>

<AnchorPane id="AnchorPane" onKeyPressed="#onKeyPressed" styleClass="Game" stylesheets="@../guisrc/blindtest.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Game2Controller">
    <children>
        <BorderPane styleClass="PlayerBackground" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <VBox alignment="CENTER" prefHeight="359.0" prefWidth="400.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="lblGuess111" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Who's song is it?" textAlignment="CENTER">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblSong1" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Song 1" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblSong2" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Song 2" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblSong3" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Song 3" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblSong4" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Song 4" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblCredit" alignment="CENTER" styleClass="Game" textAlignment="CENTER" textFill="#f9ff00">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <padding>
                                <Insets top="20.0" />
                            </padding>
                        </Label>
                        <Label fx:id="lblScores" alignment="CENTER" styleClass="Game" text="0 : 0" textAlignment="CENTER" textFill="#f9ff00">
                            <font>
                                <Font size="18.0" />
                            </font>
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </Label>
                    </children>
                </VBox>
            </center>
            <top>
                <ToolBar nodeOrientation="RIGHT_TO_LEFT" BorderPane.alignment="CENTER">
                    <items>
                        <Button fx:id="btExit" mnemonicParsing="false" onAction="#handleBtnExit" prefWidth="130.0" styleClass="StartButtons" text="End Game">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                            <graphic>
                                <ImageView fitHeight="28.0" fitWidth="28.0" preserveRatio="true">
                                    <image>
                                        <Image url="@../guisrc/mr_stop.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane minWidth="20.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="btNext" mnemonicParsing="false" onAction="#playNext" prefWidth="500.0" styleClass="StartButtons" text="Play the next Song, Sam - we do not play it again..." textAlignment="CENTER">
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </padding>
                            <graphic>
                                <ImageView fitHeight="28.0" fitWidth="28.0" preserveRatio="true">
                                    <image>
                                        <Image url="@../guisrc/mr_next.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </items>
                </ToolBar>
            </top>
            <left>
                <VBox alignment="CENTER" prefHeight="404.0" prefWidth="160.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="lblPlayerLeft" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Player left" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Button fx:id="btGuess1" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="1">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess2" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="2">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess3" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="3">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess4" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="4">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Pane minHeight="100.0" prefHeight="100.0" />
                    </children>
                </VBox>
            </left>
            <right>
                <VBox alignment="CENTER" prefHeight="404.0" prefWidth="160.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="lblPlayerRight" alignment="CENTER" contentDisplay="CENTER" minHeight="80.0" prefWidth="800.0" styleClass="Game" text="Player right" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Button fx:id="btGuess6" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="6">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess7" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="7">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess8" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="8">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Button fx:id="btGuess9" minHeight="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#checkAnswer" styleClass="GameButtons" text="9">
                            <VBox.margin>
                                <Insets bottom="1.0" top="1.0" />
                            </VBox.margin>
                        </Button>
                        <Pane minHeight="100.0" prefHeight="100.0" />
                    </children>
                </VBox>
            </right>
            <bottom>
                <StackPane prefHeight="155.0" BorderPane.alignment="CENTER">
                    <children>
                        <ImageView fx:id="imgCover" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
                            <StackPane.margin>
                                <Insets bottom="5.0" />
                            </StackPane.margin>
                        </ImageView>
                    </children>
                </StackPane>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
